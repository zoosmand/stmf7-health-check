{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "openocd",
      "serverpath": "openocd",
      "cwd": "${workspaceRoot}",
      "executable": "./build/vsarm_firmware.elf",
      "name": "Debug (OpenOCD)",
      "device": "STM32F767ZI",
      "v1": false,
      "svdFile": "${workspaceRoot}/STM32F767.svd",
      "armToolchainPath": "${env:VSARM}/arm-gcc/bin",
      "configFiles": [
        "${env:SHARE}/openocd/scripts/board/st_nucleo_f7.cfg"
      ],
      "swoConfig": {
        "enabled": true,
        "source": "probe",
        "swoFrequency": 1800000,
        "cpuFrequency": 216000000,
        "decoders": [
          {
            "port": 0,
            "label": "SWO output",
            "encoding": "utf8",
            "type": "console"
          }
        ]
      }
    },
    {
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "openocd",
      "serverpath": "openocd",
      "cwd": "${workspaceRoot}",
      "executable": "./build/vsarm_firmware.elf",
      "name": "Debug (OpenOCD no SWO)",
      "device": "STM32F767ZI",
      "v1": false,
      "svdFile": "${workspaceRoot}/STM32F767.svd",
      "armToolchainPath": "${env:VSARM}/arm-gcc/bin",
      "configFiles": [
        "${env:SHARE}/openocd/scripts/board/st_nucleo_f7.cfg"
      ]
    },
    {
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "stutil",
      "cwd": "${workspaceRoot}",
      "executable": "./build/vsarm_firmware.elf",
      "name": "Debug (ST-Util)",
      "device": "STM32F767ZI",
      "v1": false,
      "svdFile": "${workspaceRoot}/STM32F767.svd",
      "armToolchainPath": "${env:VSARM}/arm-gcc/bin"
    }
  ]
}